{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "includeEntryExports": true,
  "entry": [
    "src/compiler/glc.ts",
    "src/glidelite/glidelite.ts"
  ],
  "project": ["src/**"],
  "ignoreExportsUsedInFile": {
    "enum": true,
    "interface": true,
    "type": true
  },
  "ignoreDependencies": [
    "@tsconfig/node-lts",
    "@types/mocha",
    "mocha"
  ],
  "ignoreBinaries": ["glc"]
}
